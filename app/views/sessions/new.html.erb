<h1>Login</h1>

<%= form_with url: login_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>

    <%= f.submit 'Login' %><br>
   
<% end %>

<%= link_to 'Back home', root_path%>

<h3>Or Login with Google:</h3>
<%= button_to 'Google', '/auth/google_oauth2' %>